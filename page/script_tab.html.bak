<h2 id="ex2">이미지 여러개 적용</h2>
<div class="viewarea">
	<script>
		function ani_img(){
			var rolling_lyr = $(".rolling_lyr img"),
				rolling_lyr_active = $(".rolling_lyr img.active");
			if(rolling_lyr_active.index() == rolling_lyr.length-1 ){
				rolling_lyr_active.removeClass("active");
				rolling_lyr.eq(0).addClass("active");
			}else{
				rolling_lyr_active.removeClass("active").next().addClass("active");
			}
		}

		setInterval(function(){
			ani_img();
		},2000);
	</script>
	<style>
		.rolling_lyr img{display:none}
		.rolling_lyr .active{display:block;}
	</style>
