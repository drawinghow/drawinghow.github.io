<!-- toggle -->
<h2 id="toggle">toggle_btn</h2>
<div class="viewarea">
	<style>
		.toggle_btn{width:790px;height:64px;background:url(http://img.megals.co.kr/academy/2019/0704_leetshow/btn_info_open.jpg) no-repeat 0 0;font-size:0;display:inline-block;}
		.toggle_btn.on{background-image:url(http://img.megals.co.kr/academy/2019/0704_leetshow/btn_info_close.jpg);}
		.toggle_con{display:none;padding:50px;border:1px solid #666}
	</style>
	<script>
		$(document).ready(function () {
			$(".toggle_btn").on('click', function(e){
				e.preventDefault();
				var toggle_con = $(this).attr("href"),
					toggle_btn = $(this);

				if($(this).hasClass("on") === false){
					$(toggle_btn).addClass("on");
					$(toggle_con).slideDown();
				}else if($(this).hasClass("on") === true){
					$(toggle_btn).removeClass("on");
					$(toggle_con).slideUp();
				}
			});
		});
	</script>
	<div>
		<a href="#toggle_con1" class="toggle_btn">열기</a>
		<div class="toggle_con" id="toggle_con1">
			유의사항내용1
		</div>

		<a href="#toggle_con2" class="toggle_btn">열기</a>
		<div class="toggle_con" id="toggle_con2">
			유의사항내용2
		</div>
	</div>
</div>

